{
  "gemini": {
    "model": "gemini-2.5-flash-lite-preview-06-17",
    "thinking_budget": -1,
    "max_retries": 3,
    "retry_delay_base": 1.0
  },
  "processing": {
    "max_concurrent": 20,
    "rate_limit_per_minute": 900
  },
  "html_cleaning": {
    "remove_tags": ["script", "style", "nav", "header", "footer", "aside"],
    "preserve_structure": true
  },
  "output": {
    "add_headers": true,
    "separator": "---"
  },
  "logging": {
    "level": "INFO",
    "file": "logs/html2md.log",
    "max_bytes": 10485760,
    "backup_count": 5
  }
}
